<table border="1">

<tr>
    <th>Title</th>
    <th>Released</th>
    <th>Release Year</th>
    <th>ArtistName</th>
    <th>Genre</th>
</tr>

<tbody>
<% @songs.each do |song|%>
    <tr>
    <td><%= link_to song.title, song_path(song) %></td>
    <td><%= song.released %></td>
    <td><%= song.release_year %></td>
    <td><%= song.artist_name %></td>
    <td><%= song.genre %></td>
    </tr>
  
<%end%>

<tbody>

</table>

