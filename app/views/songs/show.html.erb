<h3><%= @song.title%></h3>
<p><%= @song.released%></p>
<p><%= @song.release_year%></p>
<p><%= @song.artist_name%></p>
<p><%= @song.genre%></p>

<%= form_tag song_path(@song), method: :delete do %>
    <%= submit_tag "Delete This Song"%>
<%end%>

<%= link_to "Delete This Song", song_path(@song), method: :delete, data: { confirm: "Are you sure to delete this?"} %>